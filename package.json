{
  "name": "pattern-js",
  "version": "1.1.0",
  "description": "Pocket-sized minimalist framework of common design patterns for JavaScript.",
  "main": "pattern.js",
  "directories": {
    "example": "examples"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "https://mayorgar@stash.mtvi.com/scm/btg/bento-js.git"
  },
  "keywords": [
    "bento",
    "js",
    "html5",
    "ads",
    "reporting"
  ],
  "jshintConfig": {
    "curly": true,
    "eqeqeq": true,
    "immed": true,
    "latedef": true,
    "newcap": true,
    "noarg": true,
    "sub": true,
    "undef": true,
    "boss": true,
    "eqnull": true,
    "browser": true,
    "globals": {
      "console": true,
      "Pattern": true,
      "$P": true,
      "LOGTAG": true,
      "TRUE": true,
      "FALSE": true,
      "NULL": true,
      "IS_FUNCTION": true,
      "IS_OBJECT": true,
      "EMPTY_FUNCTION": true,
      "IS_NUMBER": true,
      "EXEC_INIT_METHOD": true,
      "GET_OBJECT_IF_DEFINED": true,
      "FUNCTION_APPLY": true,
      "REMOVE_ARRAY_ITEM": true
    }
  },
  "author": "rgr-myrg",
  "license": "ISC",
  "homepage": "https://github.com/rgr-myrg/activity-js",
  "devDependencies": {
    "browser-sync": "^2.6.4",
    "browserify": "^9.0.8",
    "browserify-istanbul": "^0.2.1",
    "chalk": "^1.1.1",
    "gulp": "^3.8.11",
    "gulp-concat": "^2.5.2",
    "gulp-header": "^1.2.2",
    "gulp-jasmine": "^2.0.1",
    "gulp-jscs": "^3.0.0",
    "gulp-jshint": "^1.11.2",
    "gulp-karma": "0.0.4",
    "gulp-notify": "^2.2.0",
    "gulp-replace": "^0.5.4",
    "gulp-shell": "^0.4.2",
    "gulp-uglify": "^1.2.0",
    "gulp-util": "^3.0.6",
    "gulp-watch": "^4.3.5",
    "gulp-wrapper": "^0.1.5",
    "ink-docstrap": "^0.5.2",
    "jasmine-core": "^2.2.0",
    "jscs": "^2.1.1",
    "jsdoc": "3.3.0",
    "jsftp": "^1.3.1",
    "karma": "^0.13.1",
    "karma-browserify": "^4.1.2",
    "karma-chrome-launcher": "^0.1.8",
    "karma-jasmine": "^0.3.5",
    "karma-jasmine-jquery": "^0.1.1",
    "karma-js-coverage": "^0.4.0",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-spec-reporter": "0.0.18"
  }
}
